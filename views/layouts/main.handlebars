<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {{!-- Bootstrap CSS --}}
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    {{!-- Cloudinary JS --}}
    <script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript"></script>


    {{!-- jQuery --}}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    {{!-- CSS Stylesheet --}}
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">

    {{!-- Google Fonts --}}
    <link href="https://fonts.googleapis.com/css?family=Gloria+Hallelujah|Neucha|Yanone+Kaffeesatz" rel="stylesheet">


    {{!-- Cloudinary JS --}}
    <script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript">
    </script>

    {{!-- googleAuth --}}
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <meta name="google-signin-client_id"
        content="868739975173-dtu9g9t2eqr45njov8cio51mionkscib.apps.googleusercontent.com">

    <title>Instapotty</title>

</head>

<body>
    {{!-- NAVBAR START --}}
    <nav class="justify-content-between navbar navbar-expand-lg">
        <a class="navbar-brand" href="/" id="brand">Instapotty</a>
        <div>
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" id="searchTerm" type="search" placeholder="Search bathrooms"
                    aria-label="Search">
                <button class="btn btn-outline my-2 my-sm-0" id="searchBtn" type="submit">Search</button>
            </form>
        </div>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">

                </li>
                {{!-- When logged in this should appear with links to all user posts, etc. --}}
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="account" role="button" data-toggle="dropdown"
                        aria-haspopup="true" aria-expanded="false" data-name="">
                        {{!-- Username --}}

                    </a>

                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="/signed/joe/posts" id="my-posts">My Posts</a>
                        <a class="dropdown-item" href="/signed/joe/favorites" id="my-favs">My Favorited Posts</a>
                        <a class="dropdown-item" href="/" onclick="signOut();">Sign out</a>
                    </div>
                </li>
                {{!-- This should be replaced by the accounts when user is logged in --}}
                <li class="nav-item mr-auto">
                <li>
                    <div class="g-signin2 " data-onsuccess="onSignIn"></div>
                </li>
            </ul>

        </div>
    </nav>
    {{!-- NAVBAR END --}}

    {{!-- Feed area --}}
    <div class="container">

        {{!-- Dynamically generated body element --}}
        {{{body}}}

    </div>


    {{!-- Upload Image Form --}}
    <div class="flex-container" id="upload-form">
        <form id="submit-form">
            <button id="upload_widget" type="button" class="cloudinary-button btn">Upload Image</button>

            <div id="thumbnail">
            </div>
            <hr>

            <label>Location:</label>
            <input type="text" name="location" id="userInput" placeholder="Press Enter to Confirm"> <br>

            <input id="public" type="checkbox" name="public" value="public">
            <label>Public Bathroom?</label>

            <br>

            <label>Category</label>
            <select name="category" id="category" required>
                <option value=" Default" selected disabled>Potty Tags</option>
                <option value="humor">Humorous</option>
                <option value="luxurious">Luxurious</option>
                <option value="artistic">Artistic</option>
                <option value="witty">Witty</option>
                <option value="creepy">Creepy</option>
                <option value="dirty">Dirty</option>
                <option value="inclusive">Inclusive</optiion>
                <option value="cultClassic">Cult Classic</option>

            </select>
            <br>

            <label>Description</label>
            <textarea class="form-control" id="description" rows="3"></textarea>

        </form>
        <button type="button" class="btn" id="uploadSubmit">Upload Post</button>
    </div>

    {{!-- GOOGLE MAP AND INFO-WINDOW PIN THAT WILL GO INSIDE MODAL  --}}
    <!-- Map div -->
    <div id="map"></div>
    <!-- Place info content -->
    <div id="infowindow-content">
        <img src="" width="16" height="16" id="place-icon">
        <div id="place-name" class="title"></div><br>
        <div id="place-url"></div>
    </div>


    <button type="button" class="btn" id="test">Test</button>
    {{!-- Static files --}}
    <script src="/js/google_auth.js" type="text/javascript"></script>
    <script src="/js/google_api.js" type="text/javascript"></script>
    <script src="/js/ajax_calls.js" type="text/javascript"></script>
    {{!-- <script src="/js/interactions.js" type="text/javascript"></script> --}}




    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>



    {{!-- Google MAPS Javascript API --}}
    <script
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB_PUNv54yTvxfJAtR3M9J1WrfD2xHsYs8&libraries=places&callback=initMap"
        async defer></script>

</body>

</html>